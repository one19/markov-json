{
  "name": "markov-json",
  "version": "0.2.1",
  "description": "markov chain generator/saver/trainer",
  "keywords": [
    "markov",
    "typescript",
    "json",
    "probability",
    "chain",
    "text"
  ],
  "main": "dist/index.js",
  "repository": "git@github.com:one19/markov-json.git",
  "author": "Drew Showalter <maelstroswim@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "tsc && ava dist/test",
    "coverage": "tsc && nyc ava dist/test && nyc report --reporter=html",
    "lint": "eslint . --ext '.ts' --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "tsc -d -p . && lint-staged",
    "prepush": "snyk test && yarn test"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "git add dist"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^9.6.0",
    "ava": "^1.0.0-beta.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "nyc": "^11.8.0",
    "prettier": "^1.12.1",
    "prettier-eslint": "^8.8.1",
    "ts-node": "^6.0.0",
    "typescript": "^2.8.1",
    "typescript-eslint-parser": "^15.0.0"
  }
}
