{
  "name": "markov-json",
  "version": "1.1.7",
  "description": "simplest markov chain reader/writer/exporter",
  "keywords": [
    "markov",
    "typescript",
    "json",
    "probability",
    "chain",
    "text"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/index.js",
    "dist/src/index.d.ts",
    "dist/src/index.js.map"
  ],
  "browser": {
    "fs": false
  },
  "repository": "git@github.com:one19/markov-json.git",
  "author": "Drew Showalter <maelstroswim@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "tsc && ava dist/test",
    "fleeb": "tsc && nyc --reporter=lcovonly ava dist/test",
    "coverage": "tsc && nyc ava dist/test && nyc report --reporter=html",
    "lint": "eslint . --fix",
    "eslint-check": "eslint ."
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.3.0",
    "@typescript-eslint/parser": "^8.1.0",
    "ava": "^6.1.3",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "nyc": "^17.0.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0"
  }
}
