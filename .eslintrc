{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": 1,
    "prettier/prettier": [
      "error"
    ],
    "no-control-regex": 0,
    "no-undef": 0,
    "no-unused-vars": 1,
    "no-useless-escape": 0
  }
}